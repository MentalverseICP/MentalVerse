{
  "canisters": {
    "internet_identity": {
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "frontend": {},
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "type": "custom",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz"
    },
    "mentalverse_backend": {
      "main": "src/mentalverse_backend/main.mo",
      "type": "motoko",
      "dependencies": [
        "internet_identity",
        "mvt_token_canister",
        "secure_messaging"
      ]
    },
    "mvt_token_canister": {
      "main": "src/mvt_token_canister/main.mo",
      "type": "motoko",
      "dependencies": [
        "internet_identity"
      ]
    },
    "secure_messaging": {
      "type": "rust",
      "package": "secure_messaging",
      "candid": "src/secure_messaging/secure_messaging.did",
      "dependencies": [
        "internet_identity"
      ]
    },
    "frontend": {
      "dependencies": [
        "mentalverse_backend",
        "mvt_token_canister",
        "secure_messaging",
        "internet_identity"
      ],
      "source": [
        "./frontend_dist/"
      ],
      "type": "assets"
    },
    "admin_frontend": {
      "dependencies": [
        "mentalverse_backend",
        "mvt_token_canister",
        "secure_messaging",
        "internet_identity"
      ],
      "source": [
        "./admin_dist/"
      ],
      "type": "assets"
    },
    "waitlist_frontend": {
      "source": [
        "./waitlist_dist/"
      ],
      "type": "assets",
      "dependencies": [
        "waitlist_canister"
      ]
    },
    "waitlist_canister": {
      "main": "src/waitlist_canister/main.mo",
      "type": "motoko",
      "dependencies": [
        "internet_identity"
      ]
    }
  },
  "defaults": {
    "bitcoin": {
      "enabled": true,
      "log_level": "info",
      "nodes": [
        "127.0.0.1:18444"
      ]
    },
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}